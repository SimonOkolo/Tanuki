{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/cors-anywhere/(.*)",
      "dest": "/api/v1/$1"
    },
    {
      "src": "/api.consumet.org/(.*)",
      "dest": "/api/v2/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}
